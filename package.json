{
	"name": "evansheehan.com",
	"version": "1.0.0",
	"description": "Evan Sheehan's personal website",
	"private": true,
	"scripts": {
		"start": "run-s clean eleventy:dev --print-label",
		"build": "run-s clean eleventy:prod --print-label",
		"clean": "rm -rf _site",
		"eleventy:dev": "eleventy --serve",
		"eleventy:prod": "eleventy",
		"format": "prettier --write src/",
		"lint": "prettier --check src/",
		"sass": "sass src/_scss/style.scss:_site/css/style.css"
	},
	"author": "W. Evan Sheehan",
	"license": "AGPL-3.0-or-later",
	"repository": {
		"type": "git",
		"url": "git@github.com:darthmall/website.git"
	},
	"dependencies": {
		"@11ty/eleventy": "^1.0.2",
		"@11ty/eleventy-img": "^2.0.1",
		"@11ty/eleventy-plugin-rss": "^1.1.1",
		"@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
		"dotenv": "^8.6.0",
		"markdown-it-anchor": "^8.4.1",
		"markdown-it-footnote": "^3.0.3",
		"node-fetch": "^2.6.1",
		"node-sass-json-importer": "^4.3.0",
		"npm-run-all": "^4.1.5",
		"prettier": "2.7.1",
		"sass": "^1.32.12"
	}
}
