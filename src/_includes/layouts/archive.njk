{% extends 'layouts/base.njk' %}

{% block content %}
<main>
  <h1>{{ title }}</h1>

  {%- for post in collections[collectionName] -%}
    {% set postUrl = post.url %}
    {% set postTitle = post.data.title %}
    {% set postPubDate = post.date %}
    {% set postDescription = post.data.description %}
    {% include "partials/components/blog-entry.njk" %}
  {%- endfor -%}

</main>
{% endblock %}
