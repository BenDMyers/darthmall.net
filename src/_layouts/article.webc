---
layout: base.webc
---

<main class="h-entry">
	<page-header @text="title" :@tags="tags"></page-header>

  <div id="Skip" tabindex="-1" class="prose e-content">
		<aside webc:if="date" aria-label="publication date" style="align-self: start">
			<p>
				<time :@date="date" class="dt-published"></time>
			</p>
		</aside>
		<template @raw="content" webc:nokeep></template>
  </div>

	<web-mentions :@target="site.url + page.url"></web-mentions>
</main>
