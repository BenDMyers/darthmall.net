<header data-layout="cover" webc:root="override">
  <h1 id="Skip" tabindex="-1" class="display-1 p-name"
			style="text-align: center">
		<slot></slot>
	</h1>

	<ul webc:if="tags"
		aria-label="topics" class="type-body-3" data-layout="cluster"
		style="--space:var(--space-m)" data-cluster="center" role="list">

		<script webc:type="js">
			tags.map((t) => `<li><a href="/tags/${t}/">${t}</a></li>`).join("");
		</script>
	</ul>
</header>
